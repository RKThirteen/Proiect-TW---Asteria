
@import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@500&display=swap');
@import url('https://fonts.googleapis.com/css2?family=Lobster&display=swap');
:root
{
	--bg-color:#d8d0c5;
	--bg-color2:purple;
	--def-padding:25px;
	--color-b1:#1f1f1f;
	--color-w1:#edf0f1;
	--color-purp1:#4c0154;
	--color-purp2:#8102b0;
	--color-purp3:#5c017d;
	--color-bl1:#082657;
	--color-bl2:#031b42;
	--color-bl3:#011433;
	--color-bl4:#00122e;
}
*{
	margin:0;
	padding:0;
}
html{
	background-image:linear-gradient(rgba(4,9,30,0.7),rgba(4,9,30,0.7)),url(./2799006.jpg);
	background-size:cover;
	background-position:center;
	scroll-behavior: smooth;
	min-width:100px;
}


header{
	min-height:100vh;
	width:100%;
	position:relative;
	display:flex;
	flex-direction:column;
}

header .nvbr{
	box-sizing:border-box;
	margin: 0;
	display:flex;
	flex-direction:row;
	justify-content:space-between;
	align-items:center;
	padding: 1% 7%;
	
}

header>.nvbr
{
	font-family:"Montserrat", sans-serif;
	font-weight:500;
	font-size:16px;
	color:var(--color-w1);
	text-decoration:none;
} 
@media screen and (max-width:600px)
{
	.nvbr{
		flex-flow: row wrap;
	}
}
@media screen and (max-width:300px){
	.nvbr{
		flex-direction: column;
	}
}
.active
{
	transition: all 0.3s ease 0s;
	text-decoration:none;
	color:pink;
	font-size:1rem;
}
.bttn1
{
	font-family:"Montserrat", sans-serif;
	font-weight:500;
	font-size:1rem;
	color:var(--color-w1);
	text-decoration:none;
	padding:9px 25px;
	background-color:var(--color-b1);
	border:none;
	border-radius:50px;
	cursor:pointer;
	transition: all 0.3s ease 0s;
	text-decoration:none;
	max-width:25vw;
	text-align:center;
	overflow:hidden;
}
.bttn1:hover
{
	background-color:var(--color-w1);
	color:var(--color-b1);
}
h2{

	color:var(--color-w1);
	font-size:1.8rem;
	margin-bottom:10px;
	animation:AppearIntroMax 2.5s 1s linear;
	animation-fill-mode:forwards;
	opacity:0;
}
h2 + h2{
	animation:AppearIntroMax 2.5s 3.5s linear;
	animation-fill-mode:forwards;
}
h2>a.bttn1{
	color:red;
	margin-top:10%;
}
@media (prefers-reduced-motion){
	h2{
		animation-name:AppearIntroMaxReduced;
	}
	h2+h2{
		animation-name:AppearIntroMaxReduced;
	}
}

header>li
{
	font-family:"Montserrat", sans-serif;
	font-weight:500;
	font-size:16px;
	color:var(--color-w1);
	text-decoration:none;
}
a:not(p.special>a)
{
	font-family:"Montserrat", sans-serif;
	font-weight:500;
	font-size:16px;
	color:var(--color-w1);
	text-decoration:none;
	cursor:pointer;
}
p.special>a{
	color:red;
	text-decoration:none;
}

.logo
{
	cursor:pointer;
	width:100px;
	height:100px;

}

nav#navigation
{
text-align:left;
padding:15px;
padding-left:10px;
margin-right:auto;
list-style-type:none;

}

nav#navigation>ul>li
{
	display:inline-block;
	padding:0px 20px;
	position:relative;
}
nav#navigation>ul>li,
nav#navigation>ul>li>a:not(.active)
{
	transition: all 0.3s ease 0s;
	text-decoration:none;
	font-size:1rem;
}
nav#navigation>ul>li>a:hover
{
	display:initial;
	color:blue;
}

nav#navigation>ul>li.hover:hover .dropdown{
	display:block;
}
@media screen and (max-width:600px){
	.dropdown>li>a:hover{
		margin-bottom:100px;
	}
	.dropdown{

		z-index:100;
	}


}
.dropdown{
	display:none;
	position:absolute;
	left:16%;
	min-height:5vh;
	color:white;
	list-style-type:none;
	background:radial-gradient(var(--color-bl1),var(--color-bl2),var(--color-bl3));
	border-radius: 5% 5% 0;
}
.dropdown>li{
	padding:5px;
}
.dropdown>li>a{
	transition-duration:0.4s;
}
.dropdown>li>a:hover{
	color:blue;
}



#goexplore{
	text-align:center;	
	
}
#goexplore>h2
{
	margin-top:10%;
	white-space:initial;
}
@media screen and (max-width:600px){
	h2{
		margin-top:15%;
		padding-top:30%;
	}

}

#goexplore>a.bttn1{
	animation:AppearButton 2s 4s linear;
	animation-fill-mode:forwards;
	opacity:0;
	width:10%;
}

@keyframes AppearIntro{

	25%{
		font-size:2.5vw;
		opacity:0.25;
	}
	50%{
		font-size:3vw;
		opacity:0.5;
	}
	75%{
		font-size:3.5vw;
		opacity:0.75;
	}

	
	100%{
		font-size:3.5vw;
		opacity:1;
	}
}
@keyframes AppearIntroReduced{

	25%{
		font-size:2.5vw;
		opacity:0.25;
	}
	50%{
		font-size:3vw;
		opacity:0.5;
	}
	75%{
		font-size:3.5vw;
		opacity:0.75;
	}

	
	100%{
		font-size:3.5vw;
		opacity:1;
	}
}
@keyframes AppearIntroMax{

	25%{
		font-size:1.8rem;
		opacity:0.25;
	}
	50%{
		font-size:2.3rem;
		opacity:0.5;
	}
	75%{
		font-size:2.8rem;
		opacity:0.75;
	}

	
	100%{
		font-size:3rem;
		opacity:1;
	}
}
@keyframes AppearIntroMax1{

	25%{
		font-size:2rem;
		opacity:0.25;
	}
	50%{
		font-size:2rem;
		opacity:0.5;
	}

	
	100%{
		font-size:2rem;
		opacity:1;
	}
}
@keyframes AppearIntroMaxReduced{

	25%{
		font-size:2.5rem;
		opacity:0.25;
	}
	50%{
		font-size:3rem;
		opacity:0.5;
	}
	75%{
		font-size:3.5rem;
		opacity:0.75;
	}

	
	100%{
		font-size:4rem;
		opacity:1;
	}
}
@keyframes AppearButton{
	100%
	{
		opacity:1;
		width:55%;
	}
}

