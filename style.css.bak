@keyframes popUp /*JavaScript*/
{
	0%{
		height:0px;
		width:0px;
	}
	80%{
		height:300px;
		width:200px;
		opacity:0.5
	}
	100%{
		opacity:0.9;
	}
}
.gallcol>:nth-of-type(even)
{
	transform:rotate(5deg);
}
.gallcol>:nth-of-type(odd)

{
	transform:rotate(-5deg);
}
